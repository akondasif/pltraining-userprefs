{
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [ "7" ]
    },
    {
      "operatingsystem": "windows",
      "operatingsystemrelease": [
        "2012",
        "2012 R2"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.3.0 < 5.0.0"
    }
  ],
  "name": "pltraining-userprefs",
  "version": "1.0.19",
  "author": "pltraining",
  "summary": "Simple user userprefs management",
  "license": "Apache License, Version 2.0",
  "source": "https://github.com/puppetlabs/pltraining-userprefs",
  "project_page": "https://github.com/puppetlabs/pltraining-userprefs",
  "issues_url": "https://github.com/puppetlabs/pltraining-userprefs/issues",
  "description": "Important! This is not the module you are looking for\n    unless you are a trainer teaching the Puppet Fundamentals course!\n    - Allows users to customize their userprefs by choosing their editor & shell.",
  "dependencies": [
    { "name": "puppetlabs/chocolatey",   "version_requirement": ">= 0.8.0" },
    { "name": "puppetlabs/stdlib",       "version_requirement": ">= 0.1.6" },
    { "name": "puppetlabs/registry",     "version_requirement": ">= 1.1.3" },
    { "name": "stahnma/epel" }
  ]
}
